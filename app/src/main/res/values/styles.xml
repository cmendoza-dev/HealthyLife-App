<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="CustomCalendarViewStyle">
        <item name="android:background">@color/purple_700</item>
    </style>

    <style name="CustomCalendarHeaderTextAppearance" parent="TextAppearance.AppCompat">
        <item name="android:textColor">@color/purple_500</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="CustomCalendarDateTextAppearance" parent="TextAppearance.AppCompat">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="Widget.HealthyLife.AppWidget.Container" parent="android:Widget">
        <item name="android:id">@android:id/background</item>
        <item name="android:background">?android:attr/colorBackground</item>
    </style>

    <style name="Widget.HealthyLife.AppWidget.InnerView" parent="android:Widget">
        <item name="android:background">?android:attr/colorBackground</item>
        <item name="android:textColor">?android:attr/textColorPrimary</item>
    </style>


</resources>